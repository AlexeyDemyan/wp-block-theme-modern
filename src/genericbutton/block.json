{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "name": "customblocktheme/genericbutton",
  "title": "Custom Generic Button",
  "editorScript": "file:./index.js",
  "attributes": {
    "text": { "type": "string" },
    "size": { "type": "string", "default": "large" },
    "linkObject": {
      "type": "object",
      "default": {
        "url": ""
      }
    },
    "colorName": { "type": "string", "default": "blue" }
  }
}
